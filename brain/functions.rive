    var repeat = function (str, num) {
        return new Array(num + 1).join(str);
    }

	var today = new Date();
	p = (Math.min((today.getDay()+1), 7) / 7) * 100;
    p1 = Math.round(p/10);

	return repeat("▰",p1)+repeat("▱",10-p1)+" "+p.toFixed(2)+"%";
