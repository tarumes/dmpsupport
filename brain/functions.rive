> object release javascript
    var release = function() {
        deadline = new Date("2023-02-24T14:00:00Z"); // Deadline-Zeit in UTC
        now = new Date(); // Aktuelle Zeit
        timeUntilDeadline = deadline.getTime() - now.getTime(); // Differenz in Millisekunden
        seconds = Math.floor(timeUntilDeadline / 1000) % 60;
        minutes = Math.floor(timeUntilDeadline / (1000 * 60)) % 60;
        hours = Math.floor(timeUntilDeadline / (1000 * 60 * 60)) % 24;
        days = Math.floor(timeUntilDeadline / (1000 * 60 * 60 * 24));

        return (days + " Days " +  hours + " Hours " +  minutes + " Minutes " +  seconds + " Seconds UTC"); 
    }
    return release()
